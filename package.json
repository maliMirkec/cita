{
  "name": "cita",
  "version": "1.3.5",
  "description": "Website for CiTA d.o.o.",
  "main": "index",
  "repository": "git@bitbucket.org:maliMirkec/cita.git",
  "author": "Star Bist <malimirkeccita@gmail.com>",
  "license": "MIT",
  "scripts": {
    "headers": "cp src/_headers dist/_headers",
    "gfx": "cp -R gfx dist/gfx",
    "gzip": "gzip -9 dist/icons/favicon.ico",
    "clean": "rm -rf dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "build-clean": "yarn clean && yarn build && yarn headers && yarn gfx && yarn gzip"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "gsap": "^1.20.2",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.7.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "sass-mq": "^3.3.2",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "style-loader": "^0.18.2",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-plugin-critical": "^1.0.0",
    "yarn": "^0.27.5"
  }
}
