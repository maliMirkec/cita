  "scripts": {
    "cp_headers": "cp src/_headers dist/_headers",
    "remove-dist": "rm -rf dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "build-clean": "yarn run remove-dist && yarn run build && yarn run cp_headers"
  },
  "dependencies": {
    "compression-webpack-plugin": "^0.4.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
